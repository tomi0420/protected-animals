<header class="header">
  <div class="header_bar_front">
    <div class="header-left">
      <a href="/">タイトル</a>
    </div>
    <div class="header-right">
      <% if user_signed_in? %>
        <div class="user_nav grid-6">
          <span><%= current_user.nickname %></span>
        </div> 
      <% elsif conservation_group_signed_in? %>
        <div class="user_nav grid-6">
          <span><%= current_conservation_group.nickname %></span>
        </div>
      <% end %>
    </div>
  </div>
</header>